    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "stripe": "^11.14.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv-safe": "^8.1.2",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "18.11.18",
    "@types/node-uuid": "^0.0.29",
    "@types/nodemailer": "^6.4.7",
